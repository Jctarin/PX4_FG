<?xml version="1.0"?>
<PropertyList>
<generic>
   <output>
      <line_separator>newline</line_separator>
      <var_separator>,</var_separator>
 
      <chunk>
	<name>latitude</name>
  	<node>/position/latitude-deg</node>
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk> 

      <chunk>
	<name>longitude</name>
  	<node>/position/longitude-deg</node>
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk>

      <chunk>
	<name>altitude</name>
  	<node>/position/altitude-ft</node>
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk>

      <chunk>
	<name>speed - north</name>
  	<node>/velocities/speed-north-fps</node>
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk>
       
      <chunk>
	<name>speed - east</name>
  	<node>/velocities/speed-east-fps</node>
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk>
       
      <chunk>
	<name>speed - down</name>
  	<node>/velocities/speed-down-fps</node>
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk>

      <chunk>
	<name>x-accel</name>
  	<node>/accelerations/pilot/x-accel-fps_sec</node>
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk>
    
      <chunk>
	<name>y-accel</name>
  	<node>/accelerations/pilot/y-accel-fps_sec</node>
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk>  

      <chunk>
	<name>z-accel</name>
  	<node>/accelerations/pilot/z-accel-fps_sec</node>      
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk> 
       
      <chunk>
	<name>roll-rate</name>
  	<node>/orientation/roll-rate-degps</node>
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk> 

      <chunk>
	<name>pitch-rate</name>
  	<node>/orientation/pitch-rate-degps</node>
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk>   

      <chunk>
	<name>yaw-rate</name>
  	<node>/orientation/yaw-rate-degps</node>
  	<type>float</type>
  	<format>%+010.5f</format>
      </chunk>  


      <chunk>
	    <name>elapsed_sec</name>
	    <type>double</type>
        <format>%+010.6f</format>
	    <node>/sim/time/elapsed-sec</node>
      </chunk>    

      <chunk>
	<name>temp</name>
  	<node>/sim/enviroment/temperature-degc</node>
  	<type>int</type>
      </chunk> 


    	<chunk>
	<name>pressure-alt-ft</name>
	<type>double</type>
        <format>%.3f</format>
	<node>/instrumentation/altimeter/pressure-alt-ft</node>
	</chunk> 


     <chunk>
	<name>pressure-inhg</name>
	<type>double</type>
        <format>%.3f</format>
	<node>/systems/static/pressure-inhg</node>
     </chunk> 

     <chunk>
	<name>measured-total-pressure-inhg</name>
	<type>double</type>
        <format>%.3f</format>
	<node>/systems/pitot/measured-total-pressure-inhg</node>
	</chunk>

	<chunk>
	    <name>roll_deg</name>
	    <type>double</type>
        <format>%.3f</format>
	    <node>/orientation/roll-deg</node>
      </chunk> 

      <chunk>
	    <name>pitch_deg</name>
	    <type>double</type>
        <format>%.3f</format>
	    <node>/orientation/pitch-deg</node>
      </chunk> 

      <chunk>
	    <name>heading_deg</name>
	    <type>double</type>
        <format>%.3f</format>
	    <node>/orientation/heading-deg</node>
      </chunk>  

   </output>
      
</generic>
</PropertyList>
